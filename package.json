{
  "name": "socless",
  "config": {
    "aws_profile": "default",
    "dev": "--stage dev --region us-west-2",
    "stage": "--stage stage --region us-east-2",
    "prod": "--stage prod --region us-east-1",
    "sandbox": "--stage sandbox --region us-west-1"
  },
  "version": "1.0.0",
  "description": "Socless' core infrastructure and functionality",
  "main": "index.js",
  "scripts": {
    "test": ". venv/bin/activate && tox",
    "build-funcs": "node node_modules/lambda-packager/build/src/main.js functions -c common -o .archives/functions -u 'no-linux'",
    "build-layers": "node node_modules/lambda-packager/build/src/main.js layers -o .archives/layers -u 'no-linux'",
    "pretest": "python3 -m venv venv && . venv/bin/activate && pip3 install tox -qq",
    "posttest": "find . | grep -E '(__pycache__|.pytest_cache|.egg-info)' | xargs rm -rf",
    "predev": "$npm_execpath run build-funcs && $npm_execpath run build-layers",
    "dev": "serverless deploy $npm_package_config_dev --aws-profile $npm_package_config_aws_profile --verbose ",
    "prestage": "$npm_execpath run build-funcs && $npm_execpath run build-layers",
    "stage": "serverless deploy $npm_package_config_stage --aws-profile $npm_package_config_aws_profile --verbose",
    "preprod": "$npm_execpath run build-funcs && $npm_execpath run build-layers",
    "prod": "serverless deploy $npm_package_config_prod --aws-profile $npm_package_config_aws_profile --verbose",
    "presandbox": "$npm_execpath run build-funcs && $npm_execpath run build-layers",
    "sandbox": "serverless deploy $npm_package_config_sandbox --aws-profile $npm_package_config_aws_profile --verbose"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/twilio-labs/socless.git"
  },
  "keywords": [
    "socless",
    "security",
    "automation"
  ],
  "author": "Ubani Balogun",
  "license": "ISC",
  "devDependencies": {},
  "dependencies": {
    "serverless": "2.57.0",
    "lambda-packager": "git+https://github.com/noxasaxon/python-lambda-packager.git#4f4a9de5ad78aa8b0e3cbb1b5450720162525274",
    "sls-apb": "git+https://github.com/twilio-labs/sls-apb.git#1.4.3"
  }
}